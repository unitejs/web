{
	"extends": "tslint:recommended",
	"rulesDirectory": [],
	"rules": {
		"object-literal-sort-keys": false,
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "never"
				}
			}
		],
		"no-reference": false,
		"no-bitwise": false,
		"array-type": false,
		"no-empty": {
			"severity": "warning"
		},
		"no-empty-interface": {
			"severity": "warning"
		},
		"variable-name": [
			true,
			"allow-leading-underscore"
		]
	}
}